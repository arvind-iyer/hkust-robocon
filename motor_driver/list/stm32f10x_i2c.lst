NFlagStatus I2C_GetFlagStatus(I2C_TypeDef* I2Cx, uint32_t I2C_FLAG)
N{
N  FlagStatus bitstatus = RESET;
N  __IO uint32_t i2creg = 0, i2cxbase = 0;
X  volatile uint32_t i2creg = 0, i2cxbase = 0;
N  /* Check the parameters */
N  assert_param(IS_I2C_ALL_PERIPH(I2Cx));
X  ((void)0);
N  assert_param(IS_I2C_GET_FLAG(I2C_FLAG));
X  ((void)0);
N  /* Get the I2Cx peripheral base address */
N  i2cxbase = (*(uint32_t*)&(I2Cx));
N  
N  /* Read flag register index */
N  i2creg = I2C_FLAG >> 28;
N  
N  /* Get bit[23:0] of the flag */
N  I2C_FLAG &= FLAG_Mask;
X  I2C_FLAG &= ((uint32_t)0x00FFFFFF);
N  
N  if(i2creg != 0)
N  {
N    /* Get the I2Cx SR1 register address */
N    i2cxbase += 0x14;
N  }
N  else
N  {
N    /* Flag in I2Cx SR2 Register */
N    I2C_FLAG = (uint32_t)(I2C_FLAG >> 16);
N    /* Get the I2Cx SR2 register address */
N    i2cxbase += 0x18;
N  }
N  
N  if(((*(__IO uint32_t *)i2cxbase) & I2C_FLAG) != (uint32_t)RESET)
