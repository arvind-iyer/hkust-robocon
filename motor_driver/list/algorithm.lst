Nvoid average_filter()
N{
N	u8 index = 0;
N	u8 channel_index = 0;
N	u16 data_sum = 0;
N	for(index = 0; index < 16; index++)
N	{
N		data_sum = 0;
N		for(channel_index = 0; channel_index < 10; channel_index++)
N		{
N			data_sum += (u16)(ADC_ConvertedValue[channel_index*16+index]);
N		}
N		Filtered_Data_Buffer[index] = data_sum/16;
N	}	
N}
N
N
N/*recombine the data received from core2*/
Nvoid data_recombination()
N{
N	u8 index = 0;
N	for(index=0;index<16;index++)
N	{
N		All_Data_Buffer[index] = Filtered_Data_Buffer[index];
N	}
N	
N	/*raw data(from core2) recombination*/
N	for(index=0;index<16;index++)
N	{
N		ADC_ConvertedValue2[index] = ADC_ConvertedValue2_raw[index*2]*256+ADC_ConvertedValue2_raw[index*2+1];
N	}
N	
N	for(index=16;index<32;index++)
N	{
N		All_Data_Buffer[index] = (u16)(ADC_ConvertedValue2[index-16]);
N	}
N}
