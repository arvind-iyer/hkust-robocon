N/*linearization*/
Nvoid linearization()
N{
N	s32 temp[32];
N	u8 index = 0;
N	for(index = 0; index < 32; index++)
N	{
N		temp[index] =  (u32)(((u16)(All_Data_Buffer[index]) -stored_min_voltage[index]));
N		temp[index] = temp[index]*10000 / (stored_max_voltage[index] - stored_min_voltage[index]);
N		if(temp[index] < 0)
N			result[index] = 0;
N		else if(temp[index] > 10000)
N			result[index] = 10000;
N		else
N			result[index] = (u16)(temp[index]);
N	}
N}
N
N/*caculate the threshold to decide whether the result indicates high voltage or low voltage*/
Nvoid threshold_calculation()
N{
N	//assumption: expected threshold = 5000
N	u8 index = 0;
N	u16 min_result = 10000;	//no result can be larger than 10000
N	u16 max_result = 0; //no result can be less than 0
N	u16 min_high_result = 10000; //the minimum result > 5000
N	u8 possible_high_result_count = 0;
N  	
N	//find the min_result & max_result
N	for(index = 0; index < 32; index++)
N	{
N		//update min_result
N		if(min_result > result[index])
N		 	min_result = result[index];
N		//update max_result
N		if(max_result < result[index])
N			max_result = result[index];
N		//update min_high_result;
N		if(min_high_result > result[index] && result[index] > 5000)
N			min_high_result = result[index];
N		if(result[index] > 5000)
N			possible_high_result_count++;
N	}
N	if(possible_high_result_count == 0)	//no result > 5000
N	{
N		result_threshold = (max_result + min_result)/2; 
N		on_line = 0;
N		vague_flag = 1;	
N		//PAY ATTENTION: since in this case, we cannot determine
N		//whether the sensor bar is high above the gound or
N		//there's no white line detected, we set this vague_flag
N		//and will make a prediction.(NOT ACCURATE)
N	}
N	else if(possible_high_result_count < 16)	//less than 16 results > 5000
N	{
N	 	result_threshold = (min_high_result + min_result)/2;
N		on_line = 0;	//for sure
N		vague_flag = 0;
N	}
N	else 	//over 16 results > 5000 OBVIOUSLY
N	{
N	 	on_line = 1;
N		vague_flag = 0;
N	}
N}
N
Nvoid threshold_filter()
N{
N	u8 index = 0;
N	for(index = 0; index < 32; index++)
N	{
N		if(result[index] > result_threshold)
N			binary_result[index] = 1;
N		else
N			binary_result[index] = 0;
N	}
N}
N
N/*longest continuous subarray:
N calculate the starting and ending indices of the subarray */
Nvoid longest_continuous_subarray()
N{
N	u8 start_index = 0;
N	u8 end_index = 0;
N	u8 max_length = 0;
N	u8 index = 0;
N	for(index = 0; index < 32; index++)
N	{
N		if(binary_result[index])
N			end_index = index;
N		else
N		{
N			start_index = index + 1;
N			end_index = start_index;
N		}			
N
N		if(max_length < end_index - start_index + 1)
N		{
N			max_length = end_index - start_index + 1;
N			high_result_start = start_index;
N			high_result_end = end_index;
N		}	
N	}		
N}
